@model Lab3.Models.Person
@{
    Layout = "_Layout";
    ViewData["title"] = "Display favorite Person";

}

<h3>Your favorite Person</h3>

<form>
    <table>
        <tr>
            <td>First Name:- </td>
            <td>@Model.firstName</td>
        </tr>
        <tr>
            <td>Last Name:- </td>
            <td>@Model.lastName</td>
        </tr>
        <tr>
            <td>Age:- </td>
            <td>@Model.age</td>
        </tr>
        <tr>
            <td>Email Address:- </td>
            <td>@Model.email</td>
        </tr>
        <tr>
            <td>Description:- </td>
            <td>@Model.description</td>
        </tr>
    </table>
</form>